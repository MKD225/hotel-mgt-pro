<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="ajxRoomcalander" namespace="/roomcalander" extends="json-default">

		<action name="ajxSearch" class="com.hotel.action.RoomcalanderAction"
			method="search">
			<result type="json">
				<param name="root">roomcalanders</param>
			</result>
		</action>
		
		<action name="ajxAddOrUpdate" class="com.hotel.action.RoomcalanderAction"
			method="addOrUpdate">
			<result type="json"></result>
		</action>

	</package>

	<package name="ActRoomcalander" namespace="/roomcalander" extends="default">

		<action name="home">
			<result name="success" type="tiles">/RoomcalanderHome.tile</result>
		</action>

	</package>

</struts>